<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <!-- TOOLBAR -->
    <include
        android:id="@+id/toolbar"
        layout="@layout/ToolbarDialogLayout"
        />

    <!-- SCROLLVIEW -->
    <android.support.v4.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        >

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingTop="8dp"
            >

            <!-- ********************************************************************** -->
            <!-- CARD SEPARATOR -->
            <!-- ********************************************************************** -->
            <!-- 2. Four Winds BUILDING -->

            <!-- Four Winds Building Title -->
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                style="@style/ModRobotoRegularSoft20"
                android:text="Four Winds"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="8dp"
                      />

            <!-- Four Winds Building CardView -->
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardBackgroundColor="?attr/modCardBackgroundColor"
                card_view:cardElevation="2dp"
                card_view:cardUseCompatPadding="false"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="16dp"
                      >

                <!-- Four Winds Layout -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                          >

                    <!-- Four Winds BreakfastButton -->
                    <Button
                        android:id="@+id/FourWindsBreakfast"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Breakfast"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/FourWindsBreakfastTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   7:30 a.m.–8:30 a.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   5:45 a.m.–8:30 a.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   closed" />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Four Winds Lunch Button -->
                    <Button
                        android:id="@+id/FourWindsLunch"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Lunch"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/FourWindsLunchTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-S" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   11:30 a.m.–12:00 p.m.*" />
                            <TextView style="@style/ModTimesData"
                                android:text="   12:30 a.m.–1 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   10:45 a.m.–1:15 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   *Check Meal Card for availability" />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />
                    
                    <!-- Four Winds Dinner Button -->
                    <Button
                        android:id="@+id/FourWindsDinner"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Dinner"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/FourWindsDinnerTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-S" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   4:30 p.m.–5:15 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   7:15 p.m.–7:45 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   4:45 p.m.–6:30 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   4:15 p.m.–6:15 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <!-- ********************************************************************** -->
            <!-- CARD SEPARATOR -->
            <!-- ********************************************************************** -->
            <!-- 2. Varsity BUILDING -->

            <!-- Varsity Building Title -->
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                style="@style/ModRobotoRegularSoft20"
                android:text="Varsity"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="8dp"
                      />

            <!-- Varsity Building CardView -->
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardBackgroundColor="?attr/modCardBackgroundColor"
                card_view:cardElevation="2dp"
                card_view:cardUseCompatPadding="false"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="16dp"
                      >

                <!-- Varsity Layout -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                          >

                    <!-- Varsity BreakfastButton -->
                    <Button
                        android:id="@+id/VarsitysBreakfast"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Breakfast"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/VarsityBreakfastTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   closed" />
                            <TextView style="@style/ModTimesData"
                                android:text="   closed" />
                            <TextView style="@style/ModTimesData"
                                android:text="   6 a.m.–10:30 a.m." />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Varsity Lunch Button -->
                    <Button
                        android:id="@+id/VarsityLunch"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Lunch"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/VarsityLunchTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   11:30 a.m.–12:00 p.m.*" />
                            <TextView style="@style/ModTimesData"
                                android:text="   12:30 a.m.–1 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   10:45 a.m.–1:15 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   10:30 a.m.–3 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   *Check Meal Card for availability" />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Varsity Dinner Button -->
                    <Button
                        android:id="@+id/VarsityDinner"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Dinner"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/VarsityDinnerTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   7:15 p.m.–7:45 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   4:45 p.m.–6:30 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   4:15 p.m.–6:15 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   3 p.m.-6:30 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <!-- ********************************************************************** -->
            <!-- CARD SEPARATOR -->
            <!-- ********************************************************************** -->
            <!-- 2. Grab 'n Go BUILDING -->

            <!-- Grab 'n Go Building Title -->
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                style="@style/ModRobotoRegularSoft20"
                android:text="Grab 'n Go"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="8dp"
                      />

            <!-- Grab 'n Go Building CardView -->
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardBackgroundColor="?attr/modCardBackgroundColor"
                card_view:cardElevation="2dp"
                card_view:cardUseCompatPadding="false"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="16dp"
                      >

                <!-- Grab 'n Go Layout -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                          >

                    <!-- Grab 'n Go Lunch Button -->
                    <Button
                        android:id="@+id/GrabnGoLunch"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Lunch"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/GrabnGoLunchTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   10:45 a.m.–1:15 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Grab 'n Go Dinner Button -->
                    <Button
                        android:id="@+id/GrabnGoDinner"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Dinner"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/VarsityLunchTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   3 p.m.–6:30 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   closes at 6 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <!-- ********************************************************************** -->
            <!-- CARD SEPARATOR -->
            <!-- ********************************************************************** -->
            <!-- 2. Commons BUILDING -->

            <!-- Commons Building Title -->
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                style="@style/ModRobotoRegularSoft20"
                android:text="Commons"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="8dp"
                      />

            <!-- Commons Building CardView -->
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardBackgroundColor="?attr/modCardBackgroundColor"
                card_view:cardElevation="2dp"
                card_view:cardUseCompatPadding="false"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="16dp"
                      >

                <!-- Commons Layout -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                          >

                    <!-- Common Grounds Button -->
                    <Button
                        android:id="@+id/CommonGrounds"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Common Grounds"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/CommonGroundsTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   7 p.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   7 a.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   closed 6:30 p.m.–8 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   9 a.m.–10 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Palms Grille Button -->
                    <Button
                        android:id="@+id/PalmsGrille"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Palms Grille"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/PalmsGrilleTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   7 p.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   7 a.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   closed 6:30 p.m.–8 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   9 a.m.–10 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                    <!-- Divider Line -->
                    <View style="@style/ModDivider" />

                    <!-- Campus Store Button -->
                    <Button
                        android:id="@+id/CampusStore"
                        android:textColor="?attr/modBorderlessButtonFullSpanColor"
                        style="@style/ModBorderlessButtonFullSpan"
                        android:text="Campus Store"
                        android:gravity="left"
                              />

                    <!-- Times Container -->
                    <LinearLayout android:id="@+id/CampusStoreTimes"
                        style="@style/ModTimesContainer">

                        <!-- Day of Week Data -->
                        <LinearLayout style="@style/ModDayOfWeekDataContainer">
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sun" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="M-F" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Wed" />
                            <TextView style="@style/ModDayOfWeekData"
                                android:text="Sat" />
                        </LinearLayout>

                        <!-- Times Data -->
                        <LinearLayout style="@style/ModTimesDataContainer">
                            <TextView style="@style/ModTimesData"
                                android:text="   7 p.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   7 a.m.–10 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   closed 6:30 p.m.–8 p.m." />
                            <TextView style="@style/ModTimesData"
                                android:text="   9 a.m.–10 p.m." />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

    </android.support.v4.widget.NestedScrollView>

</LinearLayout>